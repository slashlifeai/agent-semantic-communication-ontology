@prefix : <https://ns.slashlife.ai/agent/examples/tax-backoffice#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TaxBackofficeAgent a core:Agent ;
    rdfs:label "Tax Backoffice Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:tax-backoffice-456" ;
            agent:title "Tax Backoffice Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To automate tax-related backoffice tasks, including calculation, form preparation, and submission." ;
            agent:owner "did:example:owner"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Tax Calculation", "Tax Form Preparation", "Tax Form Submission"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Tax Filing Request"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Financial Data"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "Completed Tax Forms"
            ]
        ]
    ] .

@prefix : <https://ns.slashlife.ai/agent/examples/bh-treadmill-iot-control#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ZenithTreadmillAgent a core:Agent ;
    rdfs:label "Zenith Treadmill Agent (Model Z-800)" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:zenith-treadmill-SN-123456789" ;
            agent:title "Zenith Treadmill Agent (Model Z-800)" ;
            agent:version "2.1.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To provide a secure, interoperable digital interface for controlling a Zenith treadmill and monitoring workout data." ;
            agent:owner "did:example:user-alice-123"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "StartWorkout", "StopWorkout", "SetSpeed", "SetIncline", "GetWorkoutData"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Control Command Received"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Signed JSON object with command parameters (e.g., { action: 'SetSpeed', value: 10.5 })"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "JSON object with current treadmill status (e.g., { speed: 10.5, incline: 2.0, status: 'running' })"
            ]
        ] ;
        agent:systemIntegrationAndDependencies [
            a agent:SystemIntegration ;
            agent:dependsOn <https://api.zenith-fitness.com/firmware-service>
        ] ;
        security:securityBinding [
            a security:SecurityBinding ;
            security:cryptographicBinding "JWS" ;
            security:trustModel "Delegated Authority via Owner's Signature"
        ]
    ] .
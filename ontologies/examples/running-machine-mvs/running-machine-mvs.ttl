@prefix : <https://ns.slashlife.ai/agent/examples/running-machine-mvs#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:RunningMachineAgent a core:Agent ;
    rdfs:label "Running Machine Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:running-machine-123" ;
            agent:title "Running Machine Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To provide a controllable interface for a running machine." ;
            agent:owner "did:example:owner"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Start", "Stop"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Control Command"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Start/Stop Command"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "Machine Status"
            ]
        ]
    ] .

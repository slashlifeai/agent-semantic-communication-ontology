@prefix : <https://ns.slashlife.ai/agent/examples/multi-agent-delegation-chain#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TopLevelAgent a core:Agent ;
    rdfs:label "Top Level Agent" .

:MidLevelAgent a core:Agent ;
    rdfs:label "Mid Level Agent" .

:LowLevelAgent a core:Agent ;
    rdfs:label "Low Level Agent" .

:Delegation1 a delegation:Delegation ;
    delegation:delegatedBy :TopLevelAgent ;
    delegation:delegatesTo :MidLevelAgent ;
    delegation:delegationScope "Task A" .

:Delegation2 a delegation:Delegation ;
    delegation:delegatedBy :MidLevelAgent ;
    delegation:delegatesTo :LowLevelAgent ;
    delegation:delegationScope "Sub-Task A.1" .

@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix ex: <https://ns.slashlife.ai/agent/examples/multi-agent-delegation-chain#> .

ex:Delegation1Shape
    a sh:NodeShape ;
    sh:targetNode ex:Delegation1 ;
    sh:property [
        sh:path delegation:delegatedBy ;
        sh:hasValue ex:TopLevelAgent ;
    ] ;
    sh:property [
        sh:path delegation:delegatesTo ;
        sh:hasValue ex:MidLevelAgent ;
    ] .

ex:Delegation2Shape
    a sh:NodeShape ;
    sh:targetNode ex:Delegation2 ;
    sh:property [
        sh:path delegation:delegatedBy ;
        sh:hasValue ex:MidLevelAgent ;
    ] ;
    sh:property [
        sh:path delegation:delegatesTo ;
        sh:hasValue ex:LowLevelAgent ;
    ] .

@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix ex: <https://ns.slashlife.ai/agent/examples/tee-binding#> .

ex:TEEBoundAgentShape
    a sh:NodeShape ;
    sh:targetNode ex:TEEBoundAgent ;
    sh:property [
        sh:path agent:hasProfile ;
        sh:node ex:TEEBoundAgentProfileShape ;
    ] .

ex:TEEBoundAgentProfileShape
    a sh:NodeShape ;
    sh:property [
        sh:path security:securityBinding ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node [
            a sh:NodeShape ;
            sh:property [
                sh:path security:cryptographicBinding ;
                sh:hasValue "TEE" ;
            ]
        ]
    ] .

@prefix : <https://ns.slashlife.ai/agent/examples/tee-binding#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TEEBoundAgent a core:Agent ;
    rdfs:label "TEE Bound Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:tee-bound-agent-jkl" ;
            agent:title "TEE Bound Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To demonstrate the binding of an agent to a Trusted Execution Environment (TEE)." ;
            agent:owner "did:example:owner"
        ] ;
        security:securityBinding [
            a security:SecurityBinding ;
            security:cryptographicBinding "TEE" ;
            security:trustModel "Intel-SGX"
        ]
    ] .

@prefix : <https://ns.slashlife.ai/agent/examples/supplier-verification-cross-border-payment#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix intent: <https://ns.slashlife.ai/agent/intent#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix payment: <https://ns.slashlife.ai/agent/payment#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# --- Agents --- 

:ProcurementAgent a core:Agent ;
    rdfs:label "ACME Inc. Procurement Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:acme-procurement-agent-1" ;
            agent:title "ACME Inc. Procurement Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To identify and initiate procurement needs for ACME Inc." ;
            agent:owner "did:example:acme-inc"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Procurement Initiation", "Supplier Sourcing"
        ]
    ] .

:SupplierManagementAgent a core:Agent ;
    rdfs:label "ACME Inc. Supplier Management Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:acme-supplier-mgmt-agent-1" ;
            agent:title "ACME Inc. Supplier Management Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To manage supplier relationships, including verification and payment orchestration." ;
            agent:owner "did:example:acme-inc"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Supplier Onboarding", "Contract Management", "Payment Orchestration"
        ]
    ] .

:VerificationAgent a core:Agent ;
    rdfs:label "ACME Inc. Verification Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:acme-verification-agent-1" ;
            agent:title "ACME Inc. Verification Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To verify supplier credentials and compliance." ;
            agent:owner "did:example:acme-inc"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Credential Verification", "Compliance Checking", "TEE Attestation"
        ]
    ] .

:FinanceAgent a core:Agent ;
    rdfs:label "ACME Inc. Finance Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:acme-finance-agent-1" ;
            agent:title "ACME Inc. Finance Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To manage financial transactions and ensure accurate payments." ;
            agent:owner "did:example:acme-inc"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Payment Processing", "Currency Exchange Orchestration", "Ledger Recording"
        ]
    ] .

:ForexAgent a core:Agent ;
    rdfs:label "External Forex Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:external-forex-agent-1" ;
            agent:title "External Forex Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To provide currency exchange services." ;
            agent:owner "did:example:forex-service-provider"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Currency Exchange", "Rate Discovery"
        ]
    ] .

:PaymentAgent a core:Agent ;
    rdfs:label "External SEPA Payment Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:external-sepa-payment-agent-1" ;
            agent:title "External SEPA Payment Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To execute SEPA payments." ;
            agent:owner "did:example:payment-service-provider"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "SEPA Transfer", "Payment Confirmation"
        ]
    ] .

# --- Initial Intent --- 

:ProcurementIntent a core:Intent ;
    rdfs:label "Procure Parts and Pay Berlin Precision Parts" ;
    intent:objective "Procure high-precision components from Berlin Precision Parts and pay 50,000 EUR." ;
    intent:input "https://example.com/acme/parts-spec-xyz" ;
    intent:output "Payment confirmation for 50,000 EUR and receipt of parts." .

# --- Delegation 1: Procurement to Supplier Management --- 

:Delegation_ProcurementToSupplierMgmt a delegation:Delegation ;
    delegation:delegatedBy :ProcurementAgent ;
    delegation:delegatesTo :SupplierManagementAgent ;
    delegation:delegationScope "Verify new supplier Berlin Precision Parts and orchestrate payment of 50,000 EUR for components." .

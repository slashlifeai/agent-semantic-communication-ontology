@prefix : <https://ns.slashlife.ai/agent/examples/supplier-verification#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:SupplierVerificationAgent a core:Agent ;
    rdfs:label "Supplier Verification Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:supplier-verification-789" ;
            agent:title "Supplier Verification Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To verify the credentials of a supplier, including business licenses and tax compliance." ;
            agent:owner "did:example:owner"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Business License Verification", "Tax Compliance Verification"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Supplier Verification Request"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Supplier Credentials"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "Verification Report"
            ]
        ]
    ] .

@prefix : <https://ns.slashlife.ai/agent/examples/ai-enterprise-organization#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix delegation: <https://ns.slashlife.ai/agent/delegation#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AIEnterpriseOrganizationAgent a core:Agent ;
    rdfs:label "AI Enterprise Organization Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:ai-enterprise-organization-ghi" ;
            agent:title "AI Enterprise Organization Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To manage the operations of an AI-powered enterprise, including task delegation and agent management." ;
            agent:owner "did:example:board-of-directors"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Agent Management", "Task Delegation", "Organizational Oversight"
        ]
    ] .

:DelegationToEmployee a delegation:Delegation ;
    delegation:delegatedBy :AIEnterpriseOrganizationAgent ;
    delegation:delegatesTo <https://ns.slashlife.ai/agent/examples/ai-workforce-employee#AIWorkforceEmployeeAgent> ;
    delegation:delegationScope "Data Analysis" .

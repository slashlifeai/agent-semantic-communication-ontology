@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix ex: <https://ns.slashlife.ai/agent/examples/cross-border-eudi-wallet#> .

ex:EUDIWalletAgentShape
    a sh:NodeShape ;
    sh:targetNode ex:EUDIWalletAgent ;
    sh:property [
        sh:path agent:hasProfile ;
        sh:node ex:EUDIWalletAgentProfileShape ;
    ] .

ex:EUDIWalletAgentProfileShape
    a sh:NodeShape ;
    sh:property [
        sh:path agent:coreIdentity ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node [
            a sh:NodeShape ;
            sh:property [
                sh:path agent:did ;
                sh:minCount 1 ;
                sh:maxCount 1 ;
                sh:datatype xsd:string ;
                sh:pattern "^did:example:123456789abcdefghi$" ;
            ]
        ]
    ] ;
    sh:property [
        sh:path agent:purposeAndRole ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node [
            a sh:NodeShape ;
            sh:property [
                sh:path agent:objective ;
                sh:minCount 1 ;
                sh:maxCount 1 ;
                sh:datatype xsd:string ;
            ]
        ]
    ] .

@prefix : <https://ns.slashlife.ai/agent/examples/cross-border-eudi-wallet#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix identity: <https://ns.slashlife.ai/agent/identity#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix security: <https://ns.slashlife.ai/agent/security-binding#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:EUDIWalletAgent a core:Agent ;
    rdfs:label "EUDI Wallet Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:123456789abcdefghi" ;
            agent:title "EUDI Wallet Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To provide a secure and reliable way to manage and present European Digital Identity (EUDI) credentials for cross-border authentication and data sharing." ;
            agent:owner "did:example:owner"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "EUDI Wallet Management"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Presentation Request"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Verifiable Presentation Request"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "Verifiable Presentation"
            ]
        ] ;
        agent:systemIntegrationAndDependencies [
            a agent:SystemIntegration ;
            agent:dependsOn <https://example.com/eud-wallet-service>
        ] ;
        security:securityBinding [
            a security:SecurityBinding ;
            security:cryptographicBinding "JWS" ;
            security:trustModel "W3C-VC-JWT"
        ]
    ] .

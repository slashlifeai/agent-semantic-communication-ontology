@prefix : <https://ns.slashlife.ai/agent/examples/iso-sc42-mapping#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix iso: <http://iso.org/23053#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ISOAgent a core:Agent, iso:AI_System ;
    rdfs:label "ISO SC42 Mapped Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:iso-agent-123" ;
            agent:title "ISO SC42 Mapped Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To demonstrate the mapping between the agent ontology and ISO/IEC AWI 23053." ;
            agent:owner "did:example:owner"
        ]
    ] .

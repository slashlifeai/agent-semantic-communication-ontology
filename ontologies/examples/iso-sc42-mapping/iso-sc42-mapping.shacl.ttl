@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix iso: <http://iso.org/23053#> .
@prefix ex: <https://ns.slashlife.ai/agent/examples/iso-sc42-mapping#> .

ex:ISOAgentShape
    a sh:NodeShape ;
    sh:targetNode ex:ISOAgent ;
    sh:property [
        sh:path rdf:type ;
        sh:hasValue core:Agent ;
        sh:hasValue iso:AI_System ;
    ] .

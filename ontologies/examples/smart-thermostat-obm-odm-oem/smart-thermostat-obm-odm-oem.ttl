# --- Phase 1: OEM --- 

:OEM_ProcurementIntent a core:Intent ;
    rdfs:label "OEM Order for CL-T-200 Thermostats" ;
    intent:objective "Manufacture 100,000 CL-T-200 thermostats according to design #CL-SPEC-2025." ;
    intent:input "https://example.com/connectedliving/design-specs/CL-SPEC-2025" ;
    intent:output "100,000 CL-T-200 thermostats and QC report." .

:OEM_Delegation_SalesToProduction a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechSalesAgent ;
    delegation:delegatesTo :InnovateTechProductionAgent ;
    delegation:delegationScope "Strictly manufacture 100,000 CL-T-200 thermostats as per ConnectedLiving's design #CL-SPEC-2025, with 99.8% minimum yield." .

:OEM_ManufacturingAction a core:Action ;
    rdfs:label "OEM Manufacturing of CL-T-200" ;
    core:contextOf :OEM_Delegation_SalesToProduction ;
    core:producesArtifact :OEM_QCReport .

:OEM_QCReport a core:Artifact ;
    rdfs:label "OEM Quality Control Report for CL-T-200" ;
    core:description "QC report for 100,000 CL-T-200 thermostats. Yield: 99.9%. Conforms to #CL-SPEC-2025." .

# --- Phase 2: ODM --- 

:ODM_DesignProposal a core:Artifact ;
    rdfs:label "InnovateTech IT-T-300 Thermostat Design Proposal" ;
    core:description "Proposal for a new smart thermostat (IT-T-300) with lower power consumption and predictive maintenance. Design Spec: https://example.com/innovatetech/design-specs/IT-SPEC-300" .

:ODM_ProcurementIntent a core:Intent ;
    rdfs:label "ODM Order for IT-T-300 Thermostats (ConnectedLiving EcoSense)" ;
    intent:objective "Procure 200,000 IT-T-300 thermostats, rebranded as 'ConnectedLiving EcoSense'." ;
    intent:input "https://example.com/innovatetech/design-specs/IT-SPEC-300" ;
    intent:output "200,000 ConnectedLiving EcoSense thermostats and QC report." .

:ODM_Delegation_SalesToProduction a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechSalesAgent ;
    delegation:delegatesTo :InnovateTechProductionAgent ;
    delegation:delegationScope "Manufacture 200,000 IT-T-300 thermostats based on InnovateTech's design #IT-SPEC-300, with ConnectedLiving EcoSense branding." .

# --- Phase 3: OBM --- 

:OBM_BrandLaunchIntent a core:Intent ;
    rdfs:label "InnovateHome Global Brand Launch" ;
    intent:objective "Globally launch 'InnovateHome' brand smart thermostats and achieve 500,000 online sales in year 1." ;
    intent:input "Market research, brand guidelines, product designs." ;
    intent:output "500,000 units sold, established brand presence." .

:OBM_Delegation_BrandToMarketing a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechBrandStrategyAgent ;
    delegation:delegatesTo :InnovateTechMarketingAgent ;
    delegation:delegationScope "Execute global digital marketing campaigns for InnovateHome smart thermostats." .

:OBM_Delegation_BrandToProduction a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechBrandStrategyAgent ;
    delegation:delegatesTo :InnovateTechProductionAgent ;
    delegation:delegationScope "Manufacture 500,000 InnovateHome smart thermostats for own inventory." .

:OBM_Delegation_BrandToeCommerce a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechBrandStrategyAgent ;
    delegation:delegatesTo :InnovateTecheCommerceAgent ;
    delegation:delegationScope "Manage InnovateHome online sales, payment processing, and logistics." .

:OBM_Delegation_BrandToSupplyChain a delegation:Delegation ;
    delegation:delegatedBy :InnovateTechBrandStrategyAgent ;
    delegation:delegatesTo :InnovateTechSupplyChainAgent ;
    delegation:delegationScope "Manage global supply chain and component procurement for InnovateHome production." .

:ConsumerPurchaseIntent a core:Intent ;
    rdfs:label "Consumer Purchase of InnovateHome Thermostat" ;
    intent:objective "Purchase one InnovateHome smart thermostat from the official website." ;
    intent:input "Customer order details, payment information." ;
    intent:output "InnovateHome smart thermostat delivered, payment confirmed." .

:eCommerceOrderProcessingAction a core:Action ;
    rdfs:label "eCommerce Order Processing" ;
    core:contextOf :OBM_Delegation_BrandToeCommerce ;
    core:producesArtifact :eCommerceOrderConfirmation .

:eCommerceOrderConfirmation a core:Artifact ;
    rdfs:label "InnovateHome Order Confirmation" ;
    core:description "Order #IH-20251109-001 confirmed. Payment received. Shipping initiated." .

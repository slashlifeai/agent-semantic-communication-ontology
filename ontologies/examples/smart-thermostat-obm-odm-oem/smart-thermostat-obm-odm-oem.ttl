# --- Agents --- 

# ConnectedLiving Agents (Client)
:ConnectedLivingProcurementAgent a core:Agent ;
    rdfs:label "ConnectedLiving Procurement Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:connectedliving-procurement-agent" ;
            agent:title "ConnectedLiving Procurement Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To source and procure components/products for ConnectedLiving brand." ;
            agent:owner "did:example:connectedliving-inc"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Supplier Sourcing", "Contract Negotiation", "Order Placement"
        ]
    ] .


@prefix : <https://ns.slashlife.ai/agent/examples/ai-workforce-employee#> .
@prefix agent: <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix capability: <https://ns.slashlife.ai/agent/capability#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AIWorkforceEmployeeAgent a core:Agent ;
    rdfs:label "AI Workforce Employee Agent" ;
    agent:hasProfile [
        a agent:AgentProfile ;
        agent:coreIdentity [
            a agent:AgentIdentity ;
            agent:did "did:example:ai-workforce-employee-abc" ;
            agent:title "AI Workforce Employee Agent" ;
            agent:version "1.0.0"
        ] ;
        agent:purposeAndRole [
            a agent:PurposeRole ;
            agent:objective "To perform tasks as a member of an AI workforce, such as data analysis and report generation." ;
            agent:owner "did:example:organization"
        ] ;
        agent:capabilities [
            a agent:CapabilityBundle ;
            agent:skill "Data Analysis", "Report Generation"
        ] ;
        agent:operationalTriggersAndIO [
            a agent:OperationalProfile ;
            agent:trigger [
                a agent:Trigger ;
                rdfs:label "Task Assignment"
            ] ;
            agent:input [
                a agent:InputSpec ;
                rdfs:label "Task Description"
            ] ;
            agent:output [
                a agent:OutputSpec ;
                rdfs:label "Task Result"
            ]
        ]
    ] .

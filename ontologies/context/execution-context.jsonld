{
  "@context": {
    "@vocab": "https://ns.slashlife.ai/agent/execution-context#",
    "core": "https://ns.slashlife.ai/agent/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",

    "id": "@id",
    "type": "@type",

    "ExecutionEnvironment": "ctx:ExecutionEnvironment",
    "SecurityContext": "ctx:SecurityContext",
    "InputState": "ctx:InputState",
    "OutputState": "ctx:OutputState",

    "hasEnvironment": { "@id": "ctx:hasEnvironment", "@type": "@id" },
    "hasSecurityContext": { "@id": "ctx:hasSecurityContext", "@type": "@id" },
    "inputState": { "@id": "ctx:inputState", "@type": "@id" },
    "outputState": { "@id": "ctx:outputState", "@type": "@id" },

    "envType": "ctx:envType",
    "usesTEE": { "@id": "ctx:usesTEE", "@type": "xsd:boolean" },
    "usesKeySlot": "ctx:usesKeySlot",
    "inputValue": "ctx:inputValue",
    "outputValue": "ctx:outputValue"
  }
}
